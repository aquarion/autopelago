server {
    listen      80;
    listen [::]:80;
    server_name www.larp.me larp.me api.larp.me;
    include /etc/nginx/snippets/errors.conf;
    return 410;
}


server {
    listen 443 ssl;
    listen [::]:443 ssl;
    server_name www.larp.me larp.me api.larp.me;
    include /etc/nginx/snippets/letsencrypt.conf;

    include /etc/nginx/snippets/errors.conf;
    return 410;
}
