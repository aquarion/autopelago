BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST={{ stream_delta_db_host }}
DB_PORT={{ stream_delta_db_port }}
DB_DATABASE={{ stream_delta_db_name }}
DB_USERNAME={{ stream_delta_db_user }}
DB_PASSWORD={{ stream_delta_db_password }}

SESSION_DRIVER=redis
SESSION_LIFETIME=120
