REDIS_CLIENT=phpredis
REDIS_HOST=localhost
REDIS_USERNAME={{ stream_delta_redis_username }}
REDIS_PASSWORD={{ stream_delta_redis_password }}
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
