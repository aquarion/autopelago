APP_NAME="Stream Delta"
APP_ENV=production
APP_KEY={{  stream_delta_app_key }}
APP_DEBUG=false
APP_TIMEZONE=UTC

APP_SERVER_PORT=80

#APP_URL=http://localhost

APP_URL={{ stream_delta_app_url }}

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
